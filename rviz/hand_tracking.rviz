Panels:
  - Class: rviz/Displays
    Help Height: 78
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /PointCloud21
        - /MarkerArray1
      Splitter Ratio: 0.5
    Tree Height: 565

Visualization Manager:
  Class: ""
  Displays:
    - Class: rviz/Axes
      Enabled: true
      Length: 1
      Name: Axes
      Radius: 0.01
      Reference Frame: camera_link
      Value: true

    - Class: rviz/Grid
      Color: 160; 160; 164
      Enabled: true
      Line Style:
        Line Width: 0.03
        Value: Lines
      Name: Grid
      Normal Cell Count: 0
      Offset:
        X: 0
        Y: 0
        Z: 0
      Plane: XY
      Plane Cell Count: 10
      Reference Frame: camera_link
      Value: true

    - Class: rviz/PointCloud2
      Enabled: true
      Name: PointCloud2
      Position Transformer: XYZ
      Queue Size: 10
      Selectable: true
      Size (Pixels): 3
      Style: Points
      Topic: /camera/depth/color/points
      Use Fixed Frame: true
      Use rainbow: true
      Value: true

    - Class: rviz/MarkerArray
      Enabled: true
      Name: MarkerArray
      Namespaces:
        hand_arrow_0: true
        hand_arrow_1: true
      Queue Size: 100
      Topic: /hand_tracking/trajectry
      Value: true

  Global Options:
    Background Color: 48; 48; 48
    Default Light: true
    Fixed Frame: camera_link
    Frame Rate: 30

  Views:
    Current:
      Class: rviz/Orbit
      Distance: 2.0
      Enable Stereo Rendering:
        Stereo Eye Separation: 0.06
        Stereo Focal Distance: 1.0
        Swap Stereo Eyes: false
        Value: false
      Focal Point:
        X: 0.0
        Y: 0.0
        Z: 0.0
      Focal Shape Fixed Size: true
      Name: Current View
      Near Clip Distance: 0.01
      Pitch: 0.0  # Look straight ahead
      Target Frame: camera_link
      Value: Orbit (rviz)
      Yaw: 3.14159  # 180 degrees to face forward

Window Geometry:
  Displays:
    collapsed: false
  Height: 846
  Hide Left Dock: false
  Hide Right Dock: false
  QMainWindow State: 
  Selection:
    collapsed: false
  Time:
    collapsed: false
  Tool Properties:
    collapsed: false
  Views:
    collapsed: false
  Width: 1267
  X: 60
  Y: 27